{
    "name": "cwp/cwp-search",
    "description": "CWP fulltextsearch integration module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "cwp/cwp": "^3",
        "silverstripe/fulltextsearch": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },
    "conflict": {
        "silverstripe/auditor": "<2.2.1"
    },
    "autoload": {
        "psr-4": {
            "CWP\\Search\\": "src/",
            "CWP\\Search\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}